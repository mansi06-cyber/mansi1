# your task is to complete this function

'''
class node:
    def __init__(data):
        self.data = data
        self.next = None
'''
class Solution:
    #  Should return data of middle node. If linked list is empty, then  -1
    def getMiddle(self, head):
        temp=head
        le=0
        mid=0
        while temp is not None:
            le+=1
            temp=temp.next
        mid=le//2
        temp=head
        for i in range(mid):
            temp=temp.next
        return temp.data
        
        
